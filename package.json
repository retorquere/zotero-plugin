{
  "name": "zotero-plugin",
  "version": "1.4.7",
  "description": "Zotero plugin builder",
  "homepage": "https://github.com/retorquere/zotero-plugin/wiki",
  "bin": {
    "zotero-plugin-release": "bin/release.js",
    "zotero-plugin-zipup": "bin/zipup.js",
    "zotero-plugin-link": "bin/link.js",
    "issue-branches": "bin/branches.js",
    "zotero-start": "bin/start.py"
  },
  "author": {
    "name": "Emiliano Heyns",
    "email": "Emiliano.Heyns@iris-advies.com"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "git push --follow-tags",
    "test": "eslint . --ext .ts && npm run build",
    "build": "tsc && chmod +x bin/*.js",
    "prepublishOnly": "npm install && npm run build",
    "go": "npm run build && npm version patch && npm publish",
    "ncu": "ncu -u && npm i && git add package.json package-lock.json && git commit -m ncu"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.4",
    "@rgrove/parse-xml": "^3.0.0",
    "@types/node": "^18.7.7",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@xmldom/xmldom": "^0.8.2",
    "ajv": "^8.11.0",
    "ajv-keywords": "^5.1.0",
    "archiver": "^5.3.1",
    "clp": "^4.0.12",
    "current-git-branch": "^1.1.0",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "glob": "^8.0.3",
    "jsesc": "^3.0.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "peggy": "^2.0.1",
    "properties-reader": "^2.2.0",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "shell-quote": "^1.7.3",
    "shelljs": "^0.8.5",
    "string-to-arraybuffer": "^1.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "uri-templates": "^0.2.0",
    "xml-parser": "^1.2.1",
    "xpath": "^0.0.32"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retorquere/zotero-plugin.git"
  },
  "license": "ISC",
  "files": [
    "bin/branches.d.ts",
    "bin/branches.js",
    "bin/link.d.ts",
    "bin/link.js",
    "bin/release.d.ts",
    "bin/release.js",
    "bin/zipup.d.ts",
    "bin/zipup.js",
    "bin/start.py",
    "continuous-integration.d.ts",
    "continuous-integration.js",
    "copy-assets.d.ts",
