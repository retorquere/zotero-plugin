    "error-report.pug",
    "install.rdf.pug",
    "loader/json.d.ts",
    "loader/json.js",
    "loader/peggy.d.ts",
    "loader/peggy.js",
    "loader/trace.d.ts",
    "loader/wrap.d.ts",
    "loader/wrap.js",
    "make-dirs.d.ts",
    "make-dirs.js",
    "package.json",
    "rdf.d.ts",
    "rdf.js",
    "root.d.ts",
    "root.js",
    ".eslintrc.js",
    "update.rdf.pug",
    "version.d.ts",
    "version.js",
