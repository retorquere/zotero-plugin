{
  "name": "zotero-plugin",
  "version": "7.0.2",
  "description": "Zotero plugin builder",
  "homepage": "https://github.com/retorquere/zotero-plugin/wiki",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./debug-log": {
      "import": "./dist/esm/utils/debug-log.js",
      "require": "./dist/cjs/utils/debug-log.cjs"
    },
    "./logger": {
      "import": "./dist/esm/utils/logger.js",
      "require": "./dist/cjs/utils/logger.cjs"
    }
  },
  "author": {
    "name": "Emiliano Heyns",
    "email": "Emiliano.Heyns@iris-advies.com"
  },
  "scripts": {
    "preversion": "npm test",
    "postversion": "git push --follow-tags",
    "test": "dprint fmt *.ts */*.ts && dprint check *.ts */*.ts && npm run build",
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "check": "tsc --noEmit",
    "start": "concurrently --raw --group 'npm run check' 'npm run build'",
    "pack": "npm test && npm pack",
    "prepublishOnly": "npm install && npm run build",
    "ncu": "ncu -u && npm i && git add package.json package-lock.json && git commit -m ncu"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@fluent/syntax": "^0.19.0",
    "@napi-rs/keyring": "^1.2.0",
    "@octokit/rest": "^22.0.0",
    "@rgrove/parse-xml": "^4.2.0",
    "@types/node": "^24.8.0",
    "@xmldom/xmldom": "^0.9.8",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "archiver": "^7.0.1",
    "clp": "^4.0.13",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.3",
    "ini": "^5.0.0",
    "jsesc": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-stream-zip": "^1.15.0",
    "openpgp": "^6.2.2",
    "peggy": "^5.0.6",
    "prompts": "^2.4.2",
    "pug": "^3.0.3",
    "rimraf": "^6.0.1",
    "shell-quote": "^1.8.3",
    "shelljs": "^0.10.0",
    "string-to-arraybuffer": "^1.0.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "uri-templates": "^0.2.0",
    "url-template": "^3.1.1",
    "uzip": "^0.20201231.0",
    "xml-parser": "^1.2.1",
    "xpath": "^0.0.34"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retorquere/zotero-plugin.git"
  },
  "license": "ISC",
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "bin": {
    "zp-release": "dist/cjs/bin/release.cjs",
    "zp-zipup": "dist/cjs/bin/zipup.cjs",
    "zp-link": "dist/cjs/bin/link.cjs",
    "issue-branches": "dist/cjs/bin/branches.cjs",
    "zotero-start": "dist/cjs/bin/start.cjs",
    "zp-fetch-log": "dist/cjs/bin/fetch-log.cjs",
    "zp-keypair": "dist/cjs/bin/keypair.cjs",
    "zp-copy-assets": "dist/cjs/copy-assets.cjs",
    "zp-make-dirs": "dist/cjs/make-dirs.cjs",
    "zp-manifest": "dist/cjs/make-manifest.cjs",
    "zp-version": "dist/cjs/version.cjs"
  },
  "bugs": {
    "url": "https://github.com/retorquere/zotero-plugin/issues"
  },
  "devDependencies": {
    "@openpgp/web-stream-tools": "^0.2.0",
    "concurrently": "^9.2.1",
    "dprint": "^0.50.2",
    "esbuild": "^0.25.11",
    "tsup": "^8.5.0"
  }
}
